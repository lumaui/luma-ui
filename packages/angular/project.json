{
  "name": "angular",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/angular/src",
  "prefix": "lib",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "dependsOn": ["^build"],
      "outputs": ["{workspaceRoot}/packages/angular/dist"],
      "options": {
        "project": "packages/angular/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "packages/angular/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "packages/angular/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/vitest:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "../../coverage/packages/angular"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    }
  }
}
